<!doctype html>
<html lang="en">
  {{> head }}

  <body>
    {{> navigation }}

    <main>
      <article>
        <header>
          <h1>{{ title }}</h1>
          <div> by {{ author }}</div>
        </header>
        {{{ contents }}}
      </article>
    
      <footer>
        {{#with previous }}<a href="/{{ path }}">previous post</a>{{/with}}
        {{#with next }}<a href="/{{ path }}">next post</a>{{/with}}
      </footer>
    </main>
  </body>
</html>
<!doctype html>
